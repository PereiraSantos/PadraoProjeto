void main() {
  Pessoa pessoa = Pessoa(name: "Marciano", idade: 30);
  Pessoa outraPessoa = Pessoa(name: "Santos", idade: 35);

  Pessoa pessoaClone = pessoa.clone();
  Pessoa outraPessoaClone = outraPessoa.clone();

  print(pessoa.toString());
  print(pessoaClone.toString());

  print(outraPessoa.toString());
  print(outraPessoaClone.toString());
}

abstract class FazClone {
  dynamic clone();
}

class Pessoa implements FazClone {
  String? name;
  int? idade;

  Pessoa({this.name, this.idade});

  @override
  clone() {
    return this;
  }

  @override
  toString() => 'Nome: $name Idade: $idade';
}
